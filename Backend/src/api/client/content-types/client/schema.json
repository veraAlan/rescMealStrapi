{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "phone": {
      "type": "biginteger"
    },
    "password": {
      "type": "password",
      "required": true,
      "maxLength": 22,
      "minLength": 5
    },
    "address": {
      "type": "text"
    },
    "birthdate": {
      "type": "date"
    }
  }
}
